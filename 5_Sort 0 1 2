#include <bits/stdc++.h> 
void sort012(int *arr, int n)
{
   //   Write your code here
   int a=0,b=0,c=0;
   for(int i=0;i<n;i++)
   {
      if(arr[i]==0)
      {
         a++;
         
      }
      else if(arr[i]==1)
      {
         b++;
         
      }
      else c++;

   }
   int j=0;
   while(a>0)
   {
      arr[j]=0;
      j++;
      a--;
   }
   while(b>0)
   {
      arr[j]=1;
      j++;
      b--;
   }
   while(c>0)
   {
      arr[j]=2;
      j++;
      c--;
   }
}
